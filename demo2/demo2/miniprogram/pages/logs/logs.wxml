<view class="body">
  <view hidden="{{logs.length != 0}}" class="nodata">暂无数据</view>
  <view class="m-t20" wx:for="{{logs}}" wx:key="index">
    <view class="time">
      <text>{{item.time}}</text>
      <text> 打卡时长: {{item.str}}</text>
    </view>
    <view class="box">
        <view class="item m-t10">
          <text class="text">{{item.date1}}</text>
          <text class="text">{{item.area1}}</text>
        </view>
        <view class="item m-t10">
          <text class="text">{{item.date2}}</text>
          <text class="text">{{item.area2}}</text>
        </view>
    </view>
  </view>



